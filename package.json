{
  "name": "country-flag-hover",
  "displayName": "Country Flag Inline",
  "description": "Display country flag emojis next to ISO country codes in your code",
  "version": "0.0.2",
  "publisher": "gautierdag",
  "repository": {
    "type": "git",
    "url": "https://github.com/gautierdag/flag-vscode-extension"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "country",
    "flag",
    "iso",
    "emoji",
    "hover"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:javascript",
    "onLanguage:plaintext",
    "onLanguage:markdown",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:csv"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}